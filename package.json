{
  "name": "nestjs-cognito-auth",
  "version": "1.0.0-0",
  "description": "NestJS Cognito Authentication Module",
  "author": "Loik Fekkai",
  "license": "MIT",
  "url": "https://github.com/Lokicoule/nestjs-cognito#readme",
  "keywords": [
    "nestjs",
    "cognito"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "coverage": "jest --coverage && coveralls < coverage/lcov.info",
    "format": "prettier --write \"{lib,tests}/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:next": "npm run build",
    "prepublish:npm": "npm run build",
    "prerelease": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "publish:npm": "npm publish --access public",
    "release": "release-it",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.105.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@golevelup/ts-jest": "^0.3.3",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "coveralls": "^3.1.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^13.0.3",
    "nyc": "^15.1.0",
    "pactum": "^3.1.11",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "./lib/**/*.(t|j)s",
      "!**/**/index.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lokicoule/nestjs-cognito"
  },
  "bugs": "https://github.com/Lokicoule/nestjs-cognito/issues"
}
